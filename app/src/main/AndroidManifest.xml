<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nacmobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SobreActivity" android:parentActivityName=".InicioActivity"/>
        <activity android:name=".InicioActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SubmarinoActivity" android:parentActivityName=".MainActivity"/>
        <activity android:name=".AmericanasActivity" android:parentActivityName=".MainActivity"/>
        <activity android:name=".NetshoesActivity" android:parentActivityName=".MainActivity"/>
        <activity android:name=".WebmotorsActivity" android:parentActivityName=".MainActivity"/>
        <activity android:name=".EbayActivity" android:parentActivityName=".MainActivity"/>
        <activity android:name=".BuscapeActivity" android:parentActivityName=".MainActivity"/>
        <activity android:name=".MercadoLivreActivity" android:parentActivityName=".MainActivity"/>
        <activity android:name=".MagaluActivity" android:parentActivityName=".MainActivity"/>
        <activity android:name=".MainActivity" android:parentActivityName=".InicioActivity"/>
    </application>

</manifest>